@model IEnumerable<FirstProject.Models.Contactu>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Phonenumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Locationmap)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phonenumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Locationmap)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Contactid">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Contactid">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Contactid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
